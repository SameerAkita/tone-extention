{
    "manifest_version": 3,
    "name": "Tone",
    "version": "0.1",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["http://localhost:3000/*"],
    "externally_connectable": ["http://localhost:3001/*"]
  }